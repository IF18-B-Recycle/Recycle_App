        android:supportsRtl="true"
        android:theme="@style/Theme.Recycle_App">
        <activity android:name=".ActivityJualBarang"></activity>
        <activity android:name=".DetailActivity" />
        <activity android:name=".Activity_About_AwalRafiHardika" />
        <activity android:name=".About.Activity_About_LukyRafiAnuggilarso" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">

        </activity>
    </application>

</manifest>